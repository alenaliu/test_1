#include<stdio.h>
int main()
{
   double a,b;
   printf("please input a and b:");
   scanf("%d %d",&a,&b);
    printf("a=%d,b=%d\n",a,b);
   printf("-a=%d,-b=%d\n",-a,-b);
   printf("a>=b =%d\n",a>=b);
   printf("a>0 && !b!=0 =%d\n",a>0 && !b!=0);
   printf("a>0,b>0 =%d\n",a>0,b>0);
   printf("a*=b =%d\n",a*=b);
   printf("sizeof(a)=%d\n",sizeof(a));
   return 0;

}
