#include<stdio.h>
int main()
{
   int a=0;
   double b=0.0;
   char c='0';
   float d=0.0;
   long e=0;
   unsigned int f=0;
   short int g=0;
   long double h=0.0;  
   printf("int               %d\n",sizeof(a));
   printf("double            %d\n",sizeof(b));
   printf("char              %d\n",sizeof(c));
   printf("float             %d\n",sizeof(d));
   printf("long              %d\n",sizeof(e));
   printf("unsigned int      %d\n",sizeof(f));
   printf("short int         %d\n",sizeof(g));
   printf("long double       %d\n",sizeof(h));
   
   return 0;
}
